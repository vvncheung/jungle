<section class="cart-show">

  <header class="page-header">
    <h1>My Cart</h1>
  </header>

  <% if !enhanced_cart.empty?%>
    <%= render 'showcart' %>
  <% else %>
      <%= link_to "Your cart is empty! Click here to go back to the home page.", {contoller: 'products'}, class: 'alert alert-success' %>
  <% end %>

</section>
